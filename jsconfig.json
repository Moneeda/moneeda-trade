{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "~~/*": [
        "./src/*"
      ],
      "@@/*": [
        "./src/*"
      ]
    },
    "types": [
      "element-plus/global"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}